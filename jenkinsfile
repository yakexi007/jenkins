pipeline {
    agent any
    environment {
        aa = "aabbcc"
    }
    stages {
        stage("build") {
            steps{
                echo "I said, Hello Mr. ${aa}"
            }
        } // end stage
        stage("test") {
            steps{
                echo "test"
            }
        } // end stage
        stage("deploy") {
            steps{
                echo "deploy"
            }
        }
    } // end stages
}